<?xml version='1.0' encoding='UTF-8'?>
<application>
	<type>Service</type>
	<public>false</public>
	
	<description>Chocolate Listener app</description>
	<name>chocolis</name>
	<lifecycleState>Deployed</lifecycleState>
	
	<urlStrategy/>
	<page-Map>
		<page>
			<name>DefaultPage</name>
			<description>This default page that will be inserted for every application. The page id of the default page will be used
	        for every page that is not registered with AR during development</description>
			
			<lifecycleState>Deployed</lifecycleState>
			<isAjax>false</isAjax>
			<isDefault>true</isDefault>
		</page>
	</page-Map>
	<webService-List>
		<webService>
			<description>Sample web service</description>
			<lifecycleState>Deployed</lifecycleState>
			<name>listener.sampleResource.v1</name>
			<svcMapping-List>
				<svcMapping>/sampleResource/v1/</svcMapping>
			</svcMapping-List>
		</webService>
	</webService-List>
	
	<secureService>true</secureService>
<parameters-Map>
<parameters key="rio.profile">Operational-Service</parameters>
</parameters-Map></application>