<?xml version='1.0' encoding='UTF-8'?>
<application>
    <type>Service</type>
    <public>false</public>
    
    <description>Marketing Collection Service</description>
    <name>mktcollectionsvc</name>
    <lifecycleState>Deployed</lifecycleState>
    <page-Map>
        <page>
            <name>DefaultPage</name>
            <description>This default page that will be inserted for every application. The page id of the default page will be used
                for every page that is not registered with AR during development</description>
            
            <lifecycleState>Deployed</lifecycleState>
            <isAjax>false</isAjax>
            <isDefault>true</isDefault>
        </page>
    </page-Map>
    <webService-List>
        <webService>
            <description>Sample web service</description>
            <lifecycleState>Deployed</lifecycleState>
            <name>mktcollectionsvc.sampleResource.v1</name>
            <svcMapping-List>
                <svcMapping>/mktcollectionsvc/v1/</svcMapping>
            </svcMapping-List>
        </webService>
    </webService-List>
    
    
<parameters-Map>
<parameters key="rio.profile">ETS-Service</parameters>
</parameters-Map></application>