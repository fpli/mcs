<?xml version="1.0" encoding="UTF-8"?>
<application>
    <type>Service</type>
    <public>false</public>
    <!-- Mandatory field -->
    <description>Required for service registration and deploy to QA/ Prod</description>
    <name>chocorprt</name>
    <lifecycleState>Deployed</lifecycleState>
    <!-- Optional field: Default value will be used if not specified -->
    <urlStrategy></urlStrategy>
    <page-Map>
        <page>
            <name>DefaultPage</name>
            <description>This default page that will be inserted for every application. The page id of the default page will be used
                for every page that is not registered with AR during development</description>
            <!-- Mandatory field -->
            <lifecycleState>Deployed</lifecycleState>
            <isAjax>false</isAjax>
            <isDefault>true</isDefault>
        </page>
    </page-Map>
    <webService-List>
        <webService>
            <description>COS Sample web service</description>
            <lifecycleState>Deployed</lifecycleState>
            <name>chocorprt.sampleapi.v1</name>
            <svcMapping-List>
                <svcMapping>/sampleapi/v1</svcMapping>
            </svcMapping-List>
        </webService>
    </webService-List>
    <!-- Optional field: true if your application invokes services that require an IAFToken -->
    <secureService>true</secureService>
    <parameters-Map>
        <parameters key="COS">true</parameters>
    </parameters-Map>
</application>
