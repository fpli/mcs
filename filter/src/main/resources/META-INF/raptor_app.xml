<?xml version="1.0" encoding="UTF-8"?>
<application>
    <type>Service</type>
    <public>false</public>
    <!-- Mandatory field -->
    <description>Chocolate Filtering service</description>
    <name>chocofil</name>
    <lifecycleState>Deployed</lifecycleState>
    <!-- Optional field: true if your application invokes services that require an IAFToken -->
    <!--<secureService>true</secureService>-->
    <page-Map>
        <page>
            <name>DefaultPage</name>
            <description>This default page that will be inserted for every application. The page id of the default page will be used for every page that is not registered with AR during development</description>
            <!-- Mandatory field -->
            <lifecycleState>Deployed</lifecycleState>
            <isAjax>false</isAjax>
            <isDefault>true</isDefault>
        </page>
    </page-Map>
    <webService-List>
        <webService>
            <description>Sample web service</description>
            <lifecycleState>Deployed</lifecycleState>
            <name>chocofil.samplesvc.v1</name>
            <svcMapping-List>
                <svcMapping>/sample/v1/</svcMapping>
            </svcMapping-List>
        </webService>
    </webService-List>
</application>